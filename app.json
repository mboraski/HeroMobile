{
    "expo": {
        "name": "HeroMobile",
        "description": "Hasty Contractor Mobile App",
        "slug": "hero-mobile",
        "privacy": "unlisted",
        "sdkVersion": "30.0.0",
        "version": "0.1.0",
        "orientation": "portrait",
        "primaryColor": "#f5a623",
        "icon": "./src/assets/icons/HastyBlackIcon.png",
        "splash": {
            "backgroundColor": "#f5a623",
            "resizeMode": "cover",
            "image": "./src/assets/splash.png"
        },
        "packagerOpts": {
            "assetExts": ["ttf", "mp4"]
        },
        "ios": {
            "bundleIdentifier": "com.spartanspeedofservice.hastyhero",
            "loadJSInBackgroundExperimental": true,
            "supportsTablet": true
        },
        "android": {
            "package": "com.spartanspeedofservice.hastyhero"
        }
    }
}
