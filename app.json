{
    "expo": {
        "name": "Hasty Hero",
        "description": "Hasty Contractor Mobile App",
        "slug": "hero-mobile",
        "privacy": "unlisted",
        "sdkVersion": "30.0.0",
        "version": "0.3.0",
        "orientation": "portrait",
        "primaryColor": "#f5a623",
        "assetBundlePatterns": [
            "src/assets/*",
            "src/assets/fonts/*",
            "src/assets/icons/*"
        ],
        "updates": {
            "enabled": true,
            "checkAutomatically": "ON_LOAD",
            "fallbackToCacheTimeout": 10000
        },
        "icon": "./src/assets/icons/HastyAppleStoreIcon.png",
        "splash": {
            "backgroundColor": "#f5a623",
            "resizeMode": "cover",
            "image": "./src/assets/splash-screen.png"
        },
        "packagerOpts": {
            "assetExts": ["ttf", "mp4"]
        },
        "ios": {
            "bundleIdentifier": "com.spartanspeedofservice.hastyhero",
            "loadJSInBackgroundExperimental": true,
            "supportsTablet": true
        },
        "android": {
            "package": "com.spartanspeedofservice.hastyhero"
        }
    }
}
