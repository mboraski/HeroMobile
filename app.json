{
    "expo": {
        "name": "Hasty Hero Mobile App",
        "description": "A delivery network and service app",
        "slug": "hero-mobile",
        "privacy": "public",
        "sdkVersion": "25.0.0",
        "version": "1.0.0",
        "orientation": "portrait",
        "primaryColor": "#f5a624",
        "icon": "./src/assets/icons/HastyOrangeIcon.png",
        "splash": {
            "backgroundColor": "#f5a624",
            "resizeMode": "cover",
            "image": "./src/assets/splash.png"
        },
        "packagerOpts": {
            "assetExts": ["ttf", "mp4"]
        },
        "ios": {
            "bundleIdentifier": "com.spartanspeedofservice.hastyhero",
            "loadJSInBackgroundExperimental": true,
            "supportsTablet": true
        }
    }
}
